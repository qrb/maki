<?xml version="1.0"?>

<node name="/de/ikkoku/sushi">
	<interface name="de.ikkoku.sushi">
		<method name="action">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="away">
			<arg name="server" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="back">
			<arg name="server" type="s" />
		</method>

		<method name="channels">
			<arg name="server" type="s" />
			<arg name="channels" type="as" direction="out" />
		</method>

		<method name="connect">
			<arg name="server" type="s" />
		</method>

		<method name="join">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="key" type="s" />
		</method>

		<method name="kick">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="who" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="nick">
			<arg name="server" type="s" />
			<arg name="nick" type="s" />
		</method>

		<method name="nicks">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nicks" type="as" direction="out" />
		</method>

		<method name="own_nick">
			<arg name="server" type="s" />
			<arg name="nick" type="s" direction="out" />
		</method>

		<method name="part">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="quit">
			<arg name="server" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="raw">
			<arg name="server" type="s" />
			<arg name="command" type="s" />
		</method>

		<method name="say">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="message" type="s" />
		</method>

		<method name="servers">
			<arg name="servers" type="as" direction="out" />
		</method>

		<method name="shutdown" />

		<method name="topic">
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="topic" type="s" />
		</method>



		<signal name="action">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nick" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="away">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
		</signal>

		<signal name="away_message">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="nick" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="back">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
		</signal>

		<signal name="connect">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
		</signal>

		<signal name="connected">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="nick" type="s" />
		</signal>

		<signal name="join">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nick" type="s" />
		</signal>

		<signal name="kick">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nick" type="s" />
			<arg name="who" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="message">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nick" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="nick">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="nick" type="s" />
			<arg name="new_nick" type="s" />
		</signal>

		<signal name="part">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="channel" type="s" />
			<arg name="nick" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="quit">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
			<arg name="nick" type="s" />
			<arg name="message" type="s" />
		</signal>

		<signal name="reconnect">
			<arg name="time" type="x" />
			<arg name="server" type="s" />
		</signal>

		<signal name="shutdown">
			<arg name="time" type="x" />
		</signal>
	</interface>
</node>
